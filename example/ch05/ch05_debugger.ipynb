    "def weighted_mean(num_list, weights):\n",
    "    running_total = 0\n",
    "    for i in range(len(num_list)):\n",
    "        running_total += (num_list[i] * weights[0]) # ❶\n",
    "    return (running_total/len(num_list))\n",
    "\n",
    "weighted_mean([1, 6, 8], [1, 3, 2])"
   ]
